{
  "name": "containerflow",
  "version": "1.0.0",
  "private": true,
  "main": "server_dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18 <23"
  },
  "scripts": {
    "//": "LOCAL DEV",
    "mobile:dev": "npx expo start -c --lan",
    "mobile:dev:tunnel": "npx expo start -c --tunnel",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "dev": "npm run server:dev",
    "//_render": "RENDER DEPLOYMENT",
    "server:build": "node esbuild.config.mjs",
    "server:start": "NODE_ENV=production node server_dist/index.js",
    "//__db": "DB",
    "db:push": "drizzle-kit push",
    "//__quality": "QUALITY",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@tanstack/react-query": "^5.90.7",
    "babel-preset-expo": "^54.0.8",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "expo": "~54.0.29",
    "expo-blur": "~15.0.8",
    "expo-camera": "^17.0.10",
    "expo-constants": "~18.0.12",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-glass-effect": "^0.1.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.10",
    "expo-location": "^19.0.8",
    "expo-sharing": "^14.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "npx": "^3.0.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-maps": "^1.20.1",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tsx": "^4.20.6",
    "ws": "^8.18.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "24.10.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.18.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.2"
  }
}
